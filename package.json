{
  "name": "eslint-plugin-vitest",
  "version": "0.0.2",
  "description": "Eslint plugin for vitest",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "vitest eslint plugin",
    "vitest eslint"
  ],
  "author": ".",
  "main": "dist/index.mjs",
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "test": "vitest --watch",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jiti": "^1.13.0",
    "prettier": "^2.5.1",
    "unbuild": "^0.7.0",
    "vitest": "^0.6.0"
  },
  "engines": {
    "node": "14.x || >= 16"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.14.0"
  }
}
